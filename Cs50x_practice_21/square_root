#include <cs50.h>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#include <unistd.h>
//Calculate the square root of a number
int main (void)
{
    double number = get_double("Enter a number to find its square root: ");
    printf("%lf ", sqrt(number));//Display answer using sqrt()
    //square(num)
}
//lf is placeholder for doubles. Generally speaking, a double can be seen as double the available space of a float.

//#####################################################
#include <cs50.h>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
#include <unistd.h>
//Calculate the square root of a number
//Next, try with your own variable
void my(double x);
int main (void)
{
    double number = get_double("Enter a number to find its square root: ");//User's number they want the square root of
    my(number);//Input the variable 'number' into your function below
}
    
    
    void my(double x)//Output is void because this functions doesn't return anything
    {
        printf("%lf ", sqrt(x));//Display result onscreen using the sqrt function
    }
