//Converting one data type to another


//Casting:Converting one data type to another

int main (void)
{
  char x = 'a';
  char y = 'b';
  char z = 'c';
  
  printf("%i %i %i ", x,y,z);

}

//Or like so:

int main (void)
{
  char x = 'a';
  char y = 'b';
  char z = 'c';
  
  printf("%i %i %i ", (int)x, (int)y, (int)z );

}
