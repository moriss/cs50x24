#include<stdio.h>
#include<cs50.h>
        //Compare two numbers. Test which is greater.
//Asks someone's name and greet them.

int main()
{

        int one=get_int("First Number");
        int two=get_int("Second Number");
        if(one<two)
        {
                printf("%i is less than %i \n", one, two);
        }
        else if(one>two)
        {
                printf("%i is greater than %i \n", one, two);
        }
        else
        {
                printf("%i is the same as %i \n", one, two);
        }
}


