B
#include<stdio.h>
#include<cs50.h>
#include<string.h>
void my();

int main()
{
        my();
}

void my()
{
        char x = get_char("Give me a letter ");
        int a = get_int("Give ma a number ");
        int i=0;
        while(i<a)
        {
                printf("%c ", x);
                i++;
        }
}

