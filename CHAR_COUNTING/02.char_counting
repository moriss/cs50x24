
A
#include<stdio.h>
#include<cs50.h>
#include<string.h>
void my();

int main()
{
        my();
}

void my()
{
        char n = get_char("Give me a letter");
        int numb=get_int("Give me a number ");
        for (int i = 0; i < numb; i++)
        {
                printf("%c " , n);
        }

