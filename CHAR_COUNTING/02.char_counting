C
// 12c Create CHAR_COUNTING to print n numbers of a char in a row.23456
#include<stdio.h>
#include<cs50.h>
#include<string.h>
void my();
int main()
{
        my();

}

void my()
{
        int a=get_int("Pick a number");
        int c=0;
        do
        {
                printf("n");
                c++;
        }
        while(c<a);
}

